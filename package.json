{
  "name": "shuffle",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:customer": "turbo run dev --filter=customer",
    "dev:panel": "turbo run dev --filter=panel"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.7",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.6"
}
